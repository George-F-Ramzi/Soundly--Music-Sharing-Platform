{
  "version": 2,
  "builds": [
    {
      "src": "build/api/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["build/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/api/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "build/api/(.*)",
      "destination": "https://soundly-nodejs.vercel.app/"
    }
  ]
}
